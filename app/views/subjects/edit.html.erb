<% provide :title, 'Edit Subject' %>
<%= form_for @subject do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.text_area :instruction%>
  </div>
  <div class="field">
    <%= f.text_field :start_date %>
  </div>
  <div class="field">
    <%= f.text_field :end_date %>
  </div>

  <%= f.submit "Edit", class: "btn btn-primary" %>
<% end %>
